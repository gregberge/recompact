{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "rules": {
    "semi": ["error", "never"],
    "react/sort-comp": "off",
    "react/require-default-props": "off",
    "react/jsx-filename-extension": ["error", {"extensions": [".js"]}],
    "react/jsx-wrap-multilines": "off",
    "import/no-extraneous-dependencies": "off"
  }
}
